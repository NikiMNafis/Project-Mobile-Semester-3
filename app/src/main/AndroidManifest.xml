<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nikimnafis.testbridgeorganizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Bridge Organizer"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestBridgeOrganizer">
        <activity android:name=".AboutActivity"></activity>
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailPriceListActivity" />
        <activity android:name=".DetailProjectActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>